train_net: "train.prototxt"
iter_size: 1

base_lr: 0.0003
#base_lr: 0.0001
#base_lr: 0.00005
#base_lr: 0.00001
#base_lr:  0.000005

momentum: 0.0
weight_decay: 0.001
lr_policy: "inv"
