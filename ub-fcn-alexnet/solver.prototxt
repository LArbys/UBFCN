train_net: "train.prototxt"
display: 1
average_loss: 20
lr_policy: "fixed"
# lr for normalized softmax
base_lr: 1e-4
# standard momentum
momentum: 0.9
# gradient accumulation
iter_size: 20
max_iter: 100000
weight_decay: 0.0005
snapshot: 4000
snapshot_prefix: "/data/vgenty/fcn"
test_initialization: false
